    <CoreFxSourcesRoot>$(MSBuildThisFileDirectory)corefx</CoreFxSourcesRoot>
    <MscorlibDir>$(MSBuildThisFileDirectory)</MscorlibDir>
    <NlpObjDir>$(BclSourcesRoot)\System\Globalization\Tables</NlpObjDir>
  </PropertyGroup>
